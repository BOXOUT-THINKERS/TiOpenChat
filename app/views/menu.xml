        </View>
        <View id="banTime" class="eachEtcBtn">
          <Label id="eactEtcFont2" class="eactEtcFont"/>
          <View class="bottomLine" />
        </View>
        <View id="helpPrivate" class="eachEtcBtn">
          <Label id="eactEtcFont3" class="eactEtcFont"/>
          <View class="bottomLine" />
        </View>
        <View id="setting" class="eachEtcBtn">
          <Label id="eactEtcFont4" class="eactEtcFont"/>
        </View>
      </View>
    </View>
    <Label id="bottomText" class="bottomText"/>

  </View>
</Alloy>
