<Alloy>
  <View class="container">
    <View class="verticalWrap">
      <View class="profileWrap">
        <View class="profileImageWrap">
          <ImageView id="profileImage"/>
          <ImageView id="profileImageOutline"/>
        </View>
        <Label id="profileName" text="test" />
        <ImageView id="editProfile"/>
      </View>

      <View class="mainBtnWrap">
        <View class="eachBtn" id="contacts">
          <Label text="contacts"/>
        </View>
        <View class="eachBtn leftPadding" id="chatList">
          <Label text="chatList"/>
        </View>
      </View>

      <View class="etcBtnWrap">
        <View class="eachEtcBtn">
          <View class="bottomLine" />
        </View>
        <View id="manageFriend" class="eachEtcBtn">
          <Label id="eactEtcFont1" class="eactEtcFont"/>
